{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(app.js|assets|index.html|style.css)",
      "dest": "/public/$1"
    },
    {
      "src": "/.*",
      "dest": "server.js"
    }
  ],
  "installCommand": "npm install"
}